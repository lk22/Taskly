                "babel-preset-env": "^1.6.1",
                "babel-preset-es2015": "^6.24.1",
                "babel-preset-latest": "^6.24.1",
                "bootstrap": "4.0.0-beta.2",
                "bootstrap-vue": "^1.3.0",
                "bulma": "^0.6.1",
                "cors": "^2.8.4",
                "expect": "^22.0.3",
